{
  "cSpell.words": ["Contentlayer", "katex", "Kode", "heroui", "Noto", "saira"],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "always"
  },
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.contentlayer": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.next": true,
    "**/.ruby-lsp": true,
    "**/.svn": true,
    "**/.vercel": true,
    "**/.yarn": true,
    "**/CVS": true,
    "**/Thumbs.db": true,
    "**/next-env.d.ts": true,
    "**/node_modules": true,
    "**/yarn.lock": true
  },
  "tailwindCSS.experimental.classRegex": [
    ["clsx\\(([^)]*)\\)", "(?:'|\"|`)([^']*)(?:'|\"|`)"]
  ],
  "commentTranslate.multiLineMerge": false,
  "typescript.tsdk": "node_modules/typescript/lib"
}
