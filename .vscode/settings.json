{
  "cSpell.words": ["Contentlayer", "katex", "Kode", "nextui", "Noto", "saira"],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "always"
  },
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.contentlayer": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.next": true,
    "**/.ruby-lsp": true,
    "**/.svn": true,
    "**/.vercel": true,
    "**/.yarn": true,
    "**/CVS": true,
    "**/Thumbs.db": true,
    "**/next-env.d.ts": true,
    "**/node_modules": true,
    "**/yarn.lock": true
  },
  "i18n-ally.editor.preferEditor": true,
  "i18n-ally.keystyle": "nested",
  "i18n-ally.localesPaths": ["./src/i18n"],
  "tailwindCSS.experimental.classRegex": [
    ["clsx\\(([^)]*)\\)", "(?:'|\"|`)([^']*)(?:'|\"|`)"]
  ]
}
